{
  "name": "elite-journal",
  "productName": "Elite Journal",
  "version": "1.9.0",
  "description": "Parsing the elite dangerous journal and putting it in a cool format!",
  "license": "MIT",
  "repository": "willyb321/elite-journal",
  "author": {
    "name": "willyb321",
    "email": "wbwilliam7@gmail.com",
    "url": "tehsuperwilly.tech"
  },
  "scripts": {
    "test": "jake test",
    "start": "set NODE_ENV=development && electron .",
    "build": "electron-zip-packager . --out=dist --overwrite --platform=win32 --arch=x64 --prune --asar",
    "pack": "jake pack",
    "dist": "jake build",
    "release": "jake release",
    "tag": "release-it",
    "lint": "xo",
    "docs": "jsdoc -d docs index.js"
  },
  "keywords": [
    "electron-app",
    "electron"
  ],
  "dependencies": {
    "about-window": "^1.6.1",
    "bugsnag": "^1.9.0",
    "debug": "^2.3.3",
    "electron-auto-updater": "^0.7.1",
    "electron-debug": "^1.1.0",
    "electron-is-dev": "^0.1.2",
    "electron-json-storage": "^2.1.0",
    "fs.extra": "^1.3.2",
    "izitoast": "^1.0.2-c",
    "jsonfile": "^2.4.0",
    "line-by-line": "^0.1.5",
    "node-watch": "^0.4.1",
    "source-map-support": "^0.4.6",
    "tableify": "^1.0.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "devtron": "^1.4.0",
    "electron": "^1.4.12",
    "electron-builder": "^10.7.0",
    "electron-zip-packager": "^4.0.0",
    "jake": "latest",
    "jsdoc": "^3.4.3",
    "mocha": "^3.2.0",
    "mocha-circleci-reporter": "^0.0.2",
    "release-it": "^2.5.1",
    "spectron": "^3.4.0",
    "xo": "^0.17.1"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "browser"
    ],
    "ignores": [
      "tests/*",
      "test.js",
      "docs/scripts/*",
      "docs/scripts/prettify/*"
    ]
  },
  "build": {
    "appId": "com.willyb321.elite-journal",
    "nsis": {
      "oneClick": true
    },
    "win": {
      "target": [
        "nsis"
      ],
      "publish": [
        "bintray",
        "github"
      ],
      "iconUrl": "https://github.com/willyb321/elite-journal/blob/master/build/icon.ico?raw=true",
      "remoteReleases": true
    }
  }
}
